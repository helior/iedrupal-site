<!DOCTYPE html>
<html lang="en">
  <head>
    {% block head %}
      <meta charset="utf-8">
      <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1">
      <meta name="generator" content="Phrozn">
      <meta name="author" content="Helior Colorado">
      <title>{% block title %}{{this.title ? "%s | "|format(this.title) : ''}}Inland Empire Drupal{% endblock %}</title>
      <link href="//cdnjs.cloudflare.com/ajax/libs/normalize/2.1.3/normalize.min.css" rel="stylesheet" type="text/css" />
      <link href="/assets/css/app.css" rel='stylesheet' type='text/css'/>
    {% endblock %}
  </head>
  <body class="page-{{this.pageId}}">
    {% block page %}
      <div class="container">
        {% include this.featuredImage ? 'featured-header.twig' : 'header.twig' %}
        <div id="content">
          {% block content %}{{ content }}{% endblock %}
        </div>
      </div>
    {% endblock %}
    {% include 'footer.twig'%}
    {% block scripts %}
      <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js" type="text/javascript"></script>
      <script src="/assets/js/app.js" type="text/javascript"></script>
    {% endblock %}

    <!--[if !(lt IE 8)]><!-->
    <script type="text/javascript">(function(){var e=document.createElement("script");e.type="text/javascript";e.async=true;e.src=document.location.protocol+"//d1agz031tafz8n.cloudfront.net/thedaywefightback.js/widget.min.js";var t=document.getElementsByTagName("script")[0];t.parentNode.insertBefore(e,t)})()</script>
    <!--<![endif]-->
  </body>
</html>
